using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using Charlotte.GameCommons;
using Charlotte.GameCommons.Options;

namespace Charlotte.Games.DungeonDesigns
{
	public class DungeonDesign_Simple : DungeonDesign
	{
		public override DDPicture GetWallPicture()
		{
			return DDCCResource.GetPicture(@"e20200003_dat\Designs\Simple0001\Wall0001.png");
		}

		public override DDPicture GetGatePicture()
		{
			return DDCCResource.GetPicture(@"e20200003_dat\Designs\Simple0001\Gate0001.png");
		}

		public override DDPicture GetBackgroundPicture()
		{
			return DDCCResource.GetPicture(@"e20200003_dat\Designs\Simple0001\Background0001.png");
		}
	}
}
