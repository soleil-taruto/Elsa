using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace Charlotte.Games.Tiles
{
	public static class TileCreator
	{
		public static Tile Create(string name)
		{
			Tile tile;

			switch (name)
			{
				case Consts.TILE_NONE: tile = new Tile_None(); break;

				// 新しいタイルをここへ追加..

				default:
					throw null; // never
			}
			return tile;
		}
	}
}
