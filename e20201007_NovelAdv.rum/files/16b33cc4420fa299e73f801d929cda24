		public DDPicture[] お菓子エフェクト = Getお菓子エフェクト().ToArray();

		private static IEnumerable<DDPicture> Getお菓子エフェクト()
		{
			DDPicture picture = DDPictureLoaders.Standard(@"e20201007_NovelAdv\ぴぽや倉庫\お菓子エフェクトアニメ\png\pipo-sweetseffect001_480.png");

			const int x_num = 5;
			const int y_num = 3;
			const int der_w = 480;
			const int der_h = 480;

			for (int y = 0; y < y_num; y++)
			{
				for (int x = 0; x < x_num; x++)
				{
					yield return DDDerivations.GetPicture(picture, x * der_w, y * der_h, der_w, der_h);
				}
			}
		}
	}
