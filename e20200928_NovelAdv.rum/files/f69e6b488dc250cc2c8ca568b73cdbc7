using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace Charlotte.Tools
{
	public struct I2Size
	{
		public int W;
		public int H;

		public I2Size(int w, int h)
		{
			this.W = w;
			this.H = h;
		}

		public D2Size ToD2Size()
		{
			return new D2Size(this.W, this.H);
		}




















	}
}
