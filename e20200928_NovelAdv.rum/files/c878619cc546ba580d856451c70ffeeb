using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace Charlotte.Tools
{
	public struct P4Poly
	{
		public D2Point LT;
		public D2Point RT;
		public D2Point RB;
		public D2Point LB;

		public P4Poly(D2Point lt, D2Point rt, D2Point rb, D2Point lb)
		{
			this.LT = lt;
			this.RT = rt;
			this.RB = rb;
			this.LB = lb;
		}








	}
}
